pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
 som:_init()
end

function _update()
 som:_update()
end

function _draw()
 som:_draw()
end

som={
 _init=function(self)
  self.state=self.title
  
  self.bubbles={}
  self.big_bubbles={}
  t=0
  for i=0,15 do
   add(self.bubbles,{s=29,x=rnd(128),y=rnd(128)})
  end
  for i=0,6 do
   local x=rnd(128)
   add(self.big_bubbles,
    {x=x,
    sx=x,
    y=rnd(128),
    r=15+rnd(10),
    sin_offset=rnd(),
    dy=-rnd(1,2)
    })
  end
  self.pl={x=64,y=64,dx=0,dy=-2,s=12}
 end,
 title={
 _update=function(self)
  if btn(5) then
   self.state=self.game
  end
 end,
 _draw=function()
  print("bubble booper",40,60,7)
  print("press —",48,66)
 end,
 },
 game={
 _update=function(self)
  --player
  local pl=self.pl
  pl.x+=pl.dx
  pl.y+=pl.dy
  pl.x=mid(pl.x,0,122)
  pl.y=max(pl.y,0)
  
  if pl.y>140 then
   --die
   sfx(9)
   self:_init()
  end
  
  --bubbles
  foreach(self.bubbles,function(bubble)
   bubble.y-=1
   if bubble.y<-10 then
    bubble.y=128
    bubble.x=rnd(128)
   end
  end)
  
  --buttons
  if btn(0) then 
   pl.dx= -2
   pl.left=true
  elseif btn(1) then 
   pl.dx=2
   pl.left=false
  else pl.dx=0
  end
  
  pl.dy+=.1
  local pop=self:closest_bubble()
  if pop then
   pl.dy=-3
   sfx(8)
   del(self.bubbles, pop)
  end
  
  pl.s=13+sgn(pl.dy)
 end,
 _draw=function(self)
  foreach(self.bubbles,self.draw_entity)
  self.draw_entity(self.pl)
 end,},
 draw_entity=function(entity)
  spr(entity.s,entity.x,entity.y,1,1,entity.left)
 end,
 distance=function(a,b)
  local x=a.x-b.x
  local y=a.y-b.y
  return sqrt(x*x+y*y)
 end,
 closest_bubble=function(self)
  local poppable
  local min_distance
  foreach(self.bubbles, function(bubble)
   local distance=self.distance(self.pl,bubble)
   if distance < 5 then
    if not poppable then
     poppable=bubble
     min_distance=distance
    else
     if distance < min_distance then
      poppable=bubble
      min_distance=distance
     end
    end 
   end
  end)
  return poppable
 end,
 
 _update=function(self)
   -- background
  foreach(self.big_bubbles,function(bubble)
   bubble.y+=bubble.dy
   bubble.x=bubble.sx+5*sin(t+bubble.sin_offset)
   if bubble.y+bubble.r<0 then
    bubble.y=150
   end
  end)
  
  self.state._update(self)
  t+=.01
 end,
 _draw=function(self)
  cls()
  rectfill(0,0,127,127,1)
  foreach(self.big_bubbles,function(bubble)
   circfill(bubble.x,bubble.y,bubble.r,13)
  end)
  self.state._draw(self)
 end
}
